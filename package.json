{
  "name": "shorttasks",
  "version": "1.1.0",
  "description": "command line shortcuts for git:g, npm: n, yarn:y. mono/multi/poly repo ready.",
  "author": "Henry Kemp",
  "license": "MIT",
  "private": false,
  "scripts": {
    "stinstall": "cd src/ && chmod +x install.sh && ./install.sh -i && cd ../..",
    "stremove": "cd src/ && chmod +x remove.sh && ./remove.sh -i && cd ../..",
    "update": "cd src/ && ./remove.sh && ./install.sh && cd ..",
    "workspace-init": "chmod +x src/workspaceinit.sh && ./src/workspaceinit.sh"
  }
}
